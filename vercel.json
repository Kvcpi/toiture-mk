{
  "version": 2,
  "buildCommand": "nuxt build",
  "devCommand": "nuxt dev",
  "framework": "nextjs",
  "builds": [
    {
      "src": "./.output/server/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "./.output/server/index.mjs"
    }
  ]
}